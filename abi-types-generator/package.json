{
  "name": "ethereum-abi-types-generator",
  "version": "1.3.2",
  "description": "Generate types from an ethereum ABI json file.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "npx tslint -c tslint.json 'src/**/*.ts'",
    "publish-cli": "node ./copy-readme.js && tsc && npm publish --access public",
    "generate-playground": "node ./dist/bin/generator-cli.js './src/__PLAYGROUND__/abi.json' --output='./src/__PLAYGROUND__' --name=playground-contract --provider=ethers_v5"
  },
  "keywords": [
    "ethereum",
    "blockchain",
    "types",
    "abi",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/joshstevens19/ethereum-abi-types-generator/issues"
  },
  "bin": {
    "abi-types-generator": "./dist/bin/generator-cli.js"
  },
  "homepage": "https://github.com/joshstevens19/ethereum-abi-types-generator#readme",
  "author": "joshstevens19@hotmail.co.uk",
  "license": "MIT",
  "dependencies": {
    "@types/bn.js": "^4.11.6",
    "bignumber.js": "^9.0.0",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "yargs": "^15.3.1",
    "ethers": "^4.0.47",
    "ethersv5": "npm:ethers@^5.0.32"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^24.0.18",
    "@types/prettier": "^2.0.0",
    "@types/yargs": "^15.0.4",
    "jest": "^24.9.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "../README.md",
    "../LICENSE"
  ]
}
